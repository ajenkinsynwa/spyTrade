# Environment variables for API keys
# Copy this to .env and fill in your actual API keys

# Alpha Vantage API (free tier available at https://www.alphavantage.co/)
ALPHA_VANTAGE_API_KEY=demo

# Finnhub API (free tier available at https://finnhub.io/)
FINNHUB_API_KEY=your_key_here

# NewsAPI (free tier available at https://newsapi.org/)
NEWS_API_KEY=your_key_here

# Application Configuration
UPDATE_INTERVAL=1800  # 30 minutes in seconds
SYMBOLS=SPY,BTC-USD
LOOKBACK_PERIOD=100  # Number of candles for historical analysis
